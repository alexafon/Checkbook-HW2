#include <iostream>
#include <string>
using namespace std;

struct Check {
	int CheckNum;
	string CheckMemo;
	float CheckAmount;

	bool operator>(float otherAmount) {
		return CheckAmount > otherAmount;
	}

	friend ostream& operator << (ostream& a, const Check& c) {
		return a << "Check #: " << c.CheckNum << ", The amount: " << c.CheckAmount << ", The memo: " << c.CheckMemo << endl;
	}
};

int main()
{
cout <<
}
